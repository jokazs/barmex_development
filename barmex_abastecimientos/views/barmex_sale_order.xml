<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="barmex_sale_order_abastecimiento" model="ir.ui.view">
        <field name="name">barmex_sale_order_abastecimiento</field>
        <field name="model">sale.order</field>
        <field name="inherit_id" ref="sale.view_order_form" />
        <field name="arch" type="xml">

            <xpath expr="//form[1]/sheet[1]/notebook[1]/page[@name='order_lines']/field[@name='order_line']/tree[1]/field[@name='product_uom_qty']" position="after">
                <field name="abastecimiento_compra" />
            </xpath>
           
        </field>
    </record>

  
</odoo>